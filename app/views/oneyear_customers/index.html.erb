<%- model_class = OneyearCustomer -%>
<div align="center">
<a href="http://rkkinfosystem.herokuapp.com/instant_customers"><button>One Time customers</button></a>
<a href="http://rkkinfosystem.herokuapp.com/twoyear_customers"><button>Two Year customers</button></a>
<a href="http://rkkinfosystem.herokuapp.com/sixmonth_customers"><button>Six Month customers</button></a>

</div>

<div align="right">
<%= form_tag :controller => 'login1', 
             :action => 'logout' do %>
<%= submit_tag 'Logout', :class => 'ctrl' %>
<% end %>
</div>
<h4><center>These customers have applied for one year service.</center></h4>
<h5><center>These Customers have given $199/Â£199</center></h5>
<br><br>
<form class="form-search">

    <%= text_field_tag :emp_no, params[:emp_no] %>

    <button type="submit" class="btn">Search by emp no</button>
    </form>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:emp_no) %></th>
      <th><%= model_class.human_attribute_name(:name) %></th>
      <th><%= model_class.human_attribute_name(:e_mail) %></th>
      <th><%= model_class.human_attribute_name(:phone) %></th>
                  <th><%= model_class.human_attribute_name(:plan) %></th>

       <th><%= model_class.human_attribute_name(:issue) %></th>
       <th><%= model_class.human_attribute_name(:trouble_shooting_steps) %></th>
      <th><%= model_class.human_attribute_name(:status) %></th>

      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
     
    </tr>
  </thead>
  <tbody>
    <% @oneyear_customers.each do |oneyear_customer| %>
      <tr>
        <td><%=  oneyear_customer.emp_no %></td>
        <td><%=  oneyear_customer.name %></td>
        <td><%=  oneyear_customer.e_mail %></td>
        <td><%=  oneyear_customer.phone %></td>
                <td><%=  oneyear_customer.plan %></td>

        <td><%= oneyear_customer.issue %></td>
        <td><%= oneyear_customer.trouble_shooting_steps %></td>
        <td><%= oneyear_customer.status %></td>
        <td><%=l oneyear_customer.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_oneyear_customer_path(oneyear_customer), :class => 'btn btn-mini' %>
          
        </td>
       
      </tr>
    <% end %>
  </tbody>
</table>

